{
  "db_name": "PostgreSQL",
  "query": "\n        select\n            id,\n            first_name,\n            last_name,\n            sex as \"sex: Sex\",\n            birth_date,\n            tax_number,\n            phone,\n            phone2,\n            email\n        from person\n        where\n            lower(first_name || ' ' || last_name) like $1\n            or lower(tax_number) like $1\n            or ($3 and regexp_replace(phone, '[^0-9]', '', 'g') like $2)\n            or ($3 and regexp_replace(coalesce(phone2, ''), '[^0-9]', '', 'g') like $2)\n            or lower(email) like $1\n        limit 30\n        ",
